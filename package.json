{
  "name": "@jonasclement/vue-accessible-select",
  "version": "1.2.1",
  "private": false,
  "description": "Vue.js component for accessible selects",
  "keywords": [
    "accessibility",
    "select",
    "vue",
    "vue-accessible-select",
    "vue-select"
  ],
  "homepage": "https://github.com/jonasclement/vue-accessible-select#readme",
  "bugs": {
    "url": "https://github.com/jonasclement/vue-accessible-select/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jonasclement/vue-accessible-select"
  },
  "license": "MIT",
  "author": "Andrew Vasilchuk <andrew.d.vasilchuk@gmail.com>",
  "files": [
    "src",
    "dist",
    "types/*.d.ts"
  ],
  "main": "dist/vue-accessible-select.js",
  "unpkg": "dist/vue-accessible-select.min.js",
  "module": "dist/vue-accessible-select.esm.js",
  "types": "types/index.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "build": "rimraf dist/* && rollup -c build/rollup.config.prod.js && npm run build:css && npm run postcss",
    "build:css": "sass src/styles:dist --no-source-map",
    "dev": "rollup -c build/rollup.config.dev.js --watch",
    "postcss": "postcss ./dist/**/*.css -r --no-map",
    "test:unit": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-replace": "^3.0.1",
    "autoprefixer": "^10.4.2",
    "babel-core": "^7.0.0-bridge.0",
    "node-sass": "npm:sass@^1.44.0",
    "postcss": "^8.4.5",
    "postcss-cli": "^9.1.0",
    "pug": "^3.0.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.63.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^4.7.2",
    "vue": "^2.6.14",
    "vue-template-compiler": "^2.6.14"
  },
  "peerDependencies": {
    "vue": "^2.6.14"
  },
  "dependencies": {
    "keycode-js": "^3.1.0"
  }
}
